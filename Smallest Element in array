import java.util.*;
public class PrimeCheck {
    public static void main(String[] args) {
       int arr[]={5,7,34,67,2,56,5,8};
       int small=arr[0];
       for(int i=0;i<arr.length;i++){
           if(small > arr[i]){
               small=arr[i];
           }
       }
       System.out.print(small);
      
    }
}
